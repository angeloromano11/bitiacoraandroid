# Android .gitignore

# Built application files
*.apk
*.ap_
*.aab

# Files for the ART/Dalvik VM
*.dex

# Java class files
*.class

# Generated files
bin/
gen/
out/
release/

# Gradle files
.gradle/
build/
**/build/

# Local configuration file (sdk path, etc)
local.properties

# Proguard folder generated by Eclipse
proguard/

# Log Files
*.log

# Android Studio Navigation editor temp files
.navigation/

# Android Studio captures folder
captures/

# IntelliJ IDEA
.idea/
*.iml
*.iws
*.ipr

# Keystore files (signing keys - NEVER commit these)
*.jks
*.keystore
*.p12
*.pem
*.key
keystore.properties
signing.properties

# Google Services (contains API keys)
google-services.json
GoogleService-Info.plist

# Firebase config
firebase.json
.firebaserc

# Secrets and API keys
secrets.properties
apikeys.properties
*.secret

# External native build folder generated in Android Studio 2.2 and later
.externalNativeBuild/
.cxx/

# NDK
obj/

# Freeline
freeline.py
freeline/
freeline_project_description.json

# fastlane
fastlane/report.xml
fastlane/Preview.html
fastlane/screenshots
fastlane/test_output
fastlane/readme.md

# Version control
vcs.xml

# lint
lint/intermediates/
lint/generated/
lint/outputs/
lint/tmp/

# Android Profiling
*.hprof

# Kotlin
.kotlin/

# OS-specific files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db
*~

# Backup files
*.bak
*.swp
*.swo

# Crash logs
crash-reports/
crash_logs/

# Test outputs
test-results/
test-reports/

# Coverage reports
coverage/
jacoco/
*.exec

# Gradle wrapper jar (some teams prefer to commit this)
# Uncomment if you don't want to commit it:
# gradle/wrapper/gradle-wrapper.jar

# Local Gradle cache
.gradle-cache/

# Room schema exports (optional - can be useful to keep for migrations)
# app/schemas/

# Sensitive data directories
app/src/main/assets/secrets/
app/src/main/res/raw/secrets/
